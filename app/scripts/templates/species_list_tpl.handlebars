<div class="especies-list-wrapper">
  <ul>
    {{#each species}}
      <li class="m-especie-item">
        <p class="especie-name">
          <a href="/#species/{{speciesId}}">{{scientificName}}{{#if commonNames}}<span> - {{commonNames}}</span>{{/if}}</a>
        </p>
        <div class="data-origin">
          {{#if gbifId}}
          <img src="images/logo_gbif.png" alt="gbif {{gbifId}}">
          {{/if}}
          {{#if speciesPlusId}}
          <img src="images/logo_species.png" alt="Species+ {{speciesPlusId}}">
          {{/if}}
          {{#if speciesId}}
          <img src="images/logo_brazil.png" alt="Brazil {{speciesId}}">
          {{/if}}
        </div>
      </li>
    {{/each}}
  </ul>
</div>
