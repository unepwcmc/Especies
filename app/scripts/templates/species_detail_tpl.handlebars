<table>
  {{#if scientificName}}
    <tr>
      <th>scientific name</th>
      <td class="first-name">{{scientificName}}</td>
      <td class="logos">
        {{#if gbifId}}
        <img src="images/logo_gbif.png" alt="gbif {{gbifId}}">
        {{/if}}
        {{#if speciesPlusId}}
        <img src="images/logo_species.png" alt="Species+ {{speciesPlusId}}">
        {{/if}}
        {{#if speciesId}}
        <img src="images/logo_brazil.png" alt="Brazil {{speciesId}}">
        {{/if}}
      </td>
    </tr>
  {{/if}}
  {{#if kingdomName}}
    <tr>
      <th>kingdom</th>
      <td>{{kingdomName}}</td>
    </tr>
  {{/if}}
  {{#if phylumName}}
    <tr>
      <th>phylum</th>
      <td>{{phylumName}}</td>
    </tr>
  {{/if}}
  {{#if className}}
    <tr>
      <th>class</th>
      <td>{{className}}</td>
    </tr>
  {{/if}}
  {{#if orderName}}
    <tr>
      <th>order</th>
      <td>{{orderName}}</td>
    </tr>
  {{/if}}
  {{#if familyName}}
    <tr>
      <th>family</th>
      <td>{{familyName}}</td>
    </tr>
  {{/if}}

  <tr></tr>

    <tr>
      <th>common names</th>
      <td>
        <ul>
        {{#each commonNames}}
          <li>{{name}}Â <span>[{{language}}]</span></li>
        {{/each}}
        </ul>
      </td>
      <td class="edition-cell"><button class="btn-edit" data="common-names">suggest changes</button></td>
    </tr>

  <tr></tr>

  <tr>
    <th>distribution</th>
    <td>
      <ul>
      {{#each distribution}}
        <li>{{region}}</li>
      {{/each}}
      </ul>
    </td>
    <td class="edition-cell"><button class="btn-edit" data="distribution">suggest changes</button></td>
  </tr>

  <tr></tr>

    <tr>
      <th>description</th>
      <td>{{description}}</td>
      <td class="edition-cell"><button class="btn-edit" data="description">suggest changes</button></td>
    </tr>

</table>





